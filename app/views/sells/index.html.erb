<p id="notice"><%= notice %></p>

<h1>Sells</h1>

<table>
  <thead>
    <tr>
      <th>Coche</th>
      <th>Comprador</th>
      <th>Vendedor</th>
      <th>Fecha publicacion</th>
      <th>Fecha compra</th>
      <th>Precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sells.each do |sell| %>
      <tr>
        <td><%= sell.coche %></td>
        <td><%= sell.comprador %></td>
        <td><%= sell.vendedor %></td>
        <td><%= sell.fecha_publicacion %></td>
        <td><%= sell.fecha_compra %></td>
        <td><%= sell.precio %></td>
        <td><%= link_to 'Show', sell %></td>
        <td><%= link_to 'Edit', edit_sell_path(sell) %></td>
        <td><%= link_to 'Destroy', sell, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sell', new_sell_path %>
