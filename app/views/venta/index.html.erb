<%= form_for coch, url (action: 'create') do |form| %>


  <div class="field">
    <%= form.label :marca %>
    <%= form.text_field :marca, id: :marca %>
  </div>

  <div class="field">
    <%= form.label :modelo %>
    <%= form.text_field :modelo, id: :modelo %>
  </div>

  <div class="field">
    <%= form.label :fecha_matriculacion %>
    <%= form.date_select :fecha_matriculacion, id: :fecha_matriculacion %>
  </div>

  <div class="field">
    <%= form.label :combustible %>
    <%= form.text_field :combustible, id: :combustible %>
  </div>

  <div class="field">
    <%= form.label :kms %>
    <%= form.number_field :kms, id: :kms %>
  </div>

  <div class="field">
    <%= form.label :descripcion %>
    <%= form.text_area :descripcion, id: :descripcion %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
