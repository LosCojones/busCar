<%= form_with(model: @coche, local: true) do |form| %>

  <div class="field form-group">
    <%= form.label :marca %>
    <%= form.text_field :marca, id: :coch_marca %>
  </div>

  <div class="field form-group">
    <%= form.label :modelo %>
    <%= form.text_field :modelo, id: :coch_modelo %>
  </div>

  <div class="field form-group">
    <%= form.label :fecha_matriculacion %>
    <%= form.date_select :fecha_matriculacion, id: :coch_fecha_matriculacion %>
  </div>

  <div class="field form-group">
    <%= form.label :combustible %>
    <%= form.text_field :combustible, id: :coch_combustible %>
  </div>

  <div class="field">
    <%= form.label :kms %>
    <%= form.number_field :kms, id: :coch_kms %>
  </div>

  <div class="field">
    <%= form.label :descripcion %>
    <%= form.text_area :descripcion, id: :coch_descripcion %>
  </div>



  <div class="actions">
    <%= form.submit %>
  </div>

<% end %>

